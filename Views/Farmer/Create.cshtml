@model PROG7311_POE_PART_2.Models.Farmer
<h2>Add Farmer</h2>

<form method="post">
    <div class="mb-3">
        <label>Name</label>
        <input asp-for="FullName" class="form-control" required />
    </div>
    <div class="mb-3">
        <label>Location</label>
        <input asp-for="Location" class="form-control" required />
